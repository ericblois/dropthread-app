{
  "expo": {
    "name": "dropthread",
    "slug": "dropthread",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ericblois.dropthread",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses your camera to take photos.",
        "NSPhotoLibraryUsageDescription": "This app uses your photos.",
        "NSLocationAlwaysUsageDescription": "This app uses your location.",
        "NSLocationWhenInUseUsageDescription": "This app uses your location.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses your location.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "localhost": {
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true,
              "NSIncludesSubdomains": true
            }
          }
        },
        "UIBackgroundModes": [
          "location",
          "remote-notification"
        ]
      },
      "config": {
        "googleMapsApiKey": "AIzaSyD-9tSrke72PouQMnMX-a7eZSW0jkFMBWY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.NOTIFICATIONS"
      ],
      "package": "com.ericblois.dropthread",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD-9tSrke72PouQMnMX-a7eZSW0jkFMBWY"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "sdkVersion": "45.0.0",
    "entryPoint": ".vscode/exponentIndex.js"
  },
  "name": "dropthread"
}